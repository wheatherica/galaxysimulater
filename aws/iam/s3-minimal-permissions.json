{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3GalaxyBucketCreate",
            "Effect": "Allow",
            "Action": [
                "s3:CreateBucket"
            ],
            "Resource": "arn:aws:s3:::galaxy-simulator-*"
        },
        {
            "Sid": "S3GalaxyObjectAccess", 
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::galaxy-simulator-*/*"
        }
    ]
}